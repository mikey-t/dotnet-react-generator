{
  "name": "dotnet-react-generator",
  "version": "0.0.5",
  "description": "A nodejs CLI util to generate a new .net react website project based on the dotnet-react-sandbox repo.",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikey-t/dotnet-react-generator"
  },
  "keywords": [
    "dotnet",
    "react",
    "generator"
  ],
  "author": "Michael Thompson",
  "license": "MIT",
  "main": "dist/src/index.js",
  "bin": "dist/src/index.js",
  "types": "dist/src/index.d.js",
  "scripts": {
    "build": "gulp build",
    "example": "node dist/src/index.js --project-name=example-project --url=example.mikeyt.net --db-name=example_mikeyt --overwrite",
    "cleanupExample": "node dist/src/index.js --cleanup-example-project true",
    "watch": "gulp watch",
    "test": "mocha || exit 0",
    "test:coverage": "nyc mocha || exit 0",
    "clean": "gulp clean",
    "pack": "gulp pack",
    "tsc": "tsc",
    "prepublish": "gulp build",
    "cloneRepoIntoTempDir": "gulp cloneRepoIntoTempDir",
    "cleanPackedTest": "gulp cleanPackedTest",
    "deps": "node dist/src/index.js --deps"
  },
  "dependencies": {
    "@mikeyt23/node-cli-utils": "^1.3.0",
    "chalk": "^4.1.2",
    "fs-extra": "^10.1.0",
    "source-map-support": "^0.5.21",
    "which": "^2.0.2",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.18",
    "@types/source-map-support": "^0.5.6",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "chai": "^4.3.6",
    "eslint": "^8.23.1",
    "gulp": "^4.0.2",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "typescript": "^4.8.3"
  }
}
